






<div class="row">
		<div class="col-md-3 ">
		     <div class="list-group ">
              <a href="#" class="list-group-item list-group-item-action active">Thông tin cá nhân</a>
              <a href="#" class="list-group-item list-group-item-action">None</a>

              
              
            </div> 
		</div>
		<div class="col-md-9">
		    <div class="card">
		        <div class="card-body">
		            <div class="row">
		                <div class="col-md-12">
		                    <h4>Thông tin cá nhân</h4>
		                    <hr>
		                </div>
		            </div>
		            <div class="row">
		                <div class="col-md-12">
		                    <form>
                              <div class="form-group row">
                                <label for="username" class="col-4 col-form-label">Tên đăng nhập :</label> 
                                <div class="col-8">
                                        <%= user.username %>
                                </div>
                              </div>
                              <div class="form-group row">
                                <label for="name" class="col-4 col-form-label">Email :</label> 
                                <div class="col-8">
                                        <%= user.email %>
                                </div>
                              </div>
                              <div class="form-group row">
                                <label for="lastname" class="col-4 col-form-label">Tên đầy đủ :</label> 
                                <div class="col-8">
                                        <%= user.fullname %>
                                </div>
                              </div>
                              <div class="form-group row">
                                <label for="text" class="col-4 col-form-label">Ngày tạo :</label> 
                                <div class="col-8">
                                        <%= user.createDate %>
                                </div>
                              </div>
                              <% if(user.agency) { %>
                              <div class="form-group row">
                                <label for="select" class="col-4 col-form-label">Tên nhà xe :</label> 
                                <div class="col-8">
                                        <%= user.agencyName %>
                                </div>
                              </div>
                              <div class="form-group row">
                                <label for="email" class="col-4 col-form-label">Địa chỉ nhà xe :</label> 
                                <div class="col-8">
                                        <%= user.agencyAdress %>
                                </div>
                              </div>
                              <div class="form-group row">
                                <label for="website" class="col-4 col-form-label">Số điện thoại nhà xe :</label> 
                                <div class="col-8">
                                        <%= user.agencyPhoneNumber %>
                                </div>
                              </div>
                              <div class="form-group row">
                                <label for="publicinfo" class="col-4 col-form-label">Mô tả nhà xe :</label> 
                                <div class="col-8">
                                        <%= user.agencyDiscription %>
                                </div>
                              </div>
                              <% } %>
                            </form>
		                </div>
		            </div>
		            
		        </div>
		    </div>
		</div>
	</div>